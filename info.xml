<?xml version="1.0"?>
<extension key="mailstorepermissions" type="module">
  <file>mailstorepermissions</file>
  <name>MailStore Permissions</name>
  <description>Change chmod parameter for processing incoming email. This can be useful, for example, to give group read access if other processes also need to access those files.
  
    Note: this requires the [Patchwork](https://github.com/artfulrobot/patchwork) extension.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/mailstorepermissions</url>
    <url desc="Documentation">https://github.com/artfulrobot/mailstorepermissions</url>
    <url desc="Support">https://github.com/artfulrobot/mailstorepermissions</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-02-07</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Mailstorepermissions</namespace>
  </civix>
</extension>
